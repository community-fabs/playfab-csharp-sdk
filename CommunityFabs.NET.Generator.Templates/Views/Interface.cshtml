@using CommunityFabs.NET.Generator.Templates.Models;
@model LegacyApiDocument;
using CommunityFabs.NET.Sdk.Models.@Model.Name;
namespace CommunityFabs.NET.Sdk.Interfaces;

public interface @($"IPlayFab{Model.Name}Api") {
@foreach (var apiCall in Model.Calls!)
{
    @(Html.Raw(apiCall.GetSummary("    /// ")))
    @Html.Raw($"    public Task<{apiCall.Result}> {apiCall.Name}Async({apiCall.Request} request);\r\n")
}
}